      # 部署mini-core 小程序二维码
      - uses: actions/checkout@v2
        with:
          repository: 'kne-union/mini-core'
          ref: 'master'
      - run: npx @kne/miniprogram-tools qrcode
        env:
          APP_ID: wxae849489ee6c1994
          SECRET: ${{secrets.KNEUNION_SECRET}}
      - run: rm -rf ./website/mini-core && mkdir -p ./website/mini-core && cp -r ./qr-code/* ./website/mini-core
      - run: rm -rf ./qr-code

